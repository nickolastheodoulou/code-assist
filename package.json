{
  "name": "code-prompt-assist",
  "publisher": "NickolasTheodoulou",
  "displayName": "code-prompt-assist",
  "description": "code prompt assist from workspace",
  "version": "1.0.7",
  "engines": {
    "vscode": "^1.0.0"
  },
  "changelog": {
    "path": "./CHANGELOG.md"
  },
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/nickolastheodoulou/code-assist.git"
  },  
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "properties": {
      "codeAssist.redactionRules": {
        "type": "array",
        "default": [],
        "items": {
          "type": "object",
          "properties": {
            "original": {
              "type": "string",
              "description": "The original string to be redacted"
            },
            "replacement": {
              "type": "string",
              "description": "Replacement string. Leave empty for default redaction."
            }
          },
          "required": [
            "original"
          ]
        },
        "description": "List of strings to redact and their replacements."
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codeAssistContainer",
          "title": "Code Prompt Assist",
          "icon": "media/icon.svg"
        }
      ]
    },
    "views": {
      "codeAssistContainer": [
        {
          "id": "codeAssistTreeView",
          "name": "Code Prompt Assist"
        }
      ]
    }
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "lint": "eslint src --ext ts",
    "test": "jest",
    "vscode:prepublish": "npm run package",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "~18.19.3",
    "@types/vscode": "^1.0.0",
    "@types/vscode-webview": "^1.57.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.55.0",
    "glob": "^10.3.10",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}